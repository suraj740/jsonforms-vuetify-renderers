{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "skipLibCheck": true,
        "strict": false,
        "declaration": false,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "importHelpers": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "sourceMap": true,
        "outDir": "./lib",
        "jsx": "preserve",
        "types": [
            "node",
            "vue",
            "vuetify",
            "jest"
        ],
        "lib": [
            "esnext",
            "dom",
            "dom.iterable",
            "scripthost"
        ]
    },
    "exclude": [
        "node_modules",
        "lib",
        "src"
    ]
}
